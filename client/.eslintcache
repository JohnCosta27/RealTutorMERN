ansparacy\n\t\t\t\t\t\t\tfrom us.\n\t\t\t\t\t\t</Typography>\n\t\t\t\t\t\t<br></br>\n\t\t\t\t\t\t<br></br>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div\n\t\t\t\tclassName={classes.smallSection}\n\t\t\t\tstyle={{ backgroundColor: teal[200] }}\n\t\t\t>\n\t\t\t\t<div\n\t\t\t\t\tclassName={classes.sectionPaper}\n\t\t\t\t\tstyle={{ alignItems: 'center' }}\n\t\t\t\t>\n\t\t\t\t\t<div style={{ width: '100%' }}>\n\t\t\t\t\t\t<Typography variant=\"h1\">\n\t\t\t\t\t\t\tHow does it all work?\n\t\t\t\t\t\t</Typography>\n\t\t\t\t\t\t<br></br>\n\n\t\t\t\t\t\t<div style={{ display: 'flex' }}>\n\t\t\t\t\t\t\t<Typography variant=\"h3\" color=\"secondary\">\n\t\t\t\t\t\t\t\t1.\n\t\t\t\t\t\t\t</Typography>\n\t\t\t\t\t\t\t<Typography variant=\"h3\">Contact us</Typography>\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t\tThis could be through email, phone, social media.\n\t\t\t\t\t\t\tGet in contact with us and we will match you to one\n\t\t\t\t\t\t\tof our tutors.\n\t\t\t\t\t\t</Typography>\n\n\t\t\t\t\t\t<br></br>\n\n\t\t\t\t\t\t<div style={{ display: 'flex' }}>\n\t\t\t\t\t\t\t<Typography variant=\"h3\" color=\"secondary\">\n\t\t\t\t\t\t\t\t2.\n\t\t\t\t\t\t\t</Typography>\n\t\t\t\t\t\t\t<Typography variant=\"h3\">\n\t\t\t\t\t\t\t\tSchedule and payment\n\t\t\t\t\t\t\t</Typography>\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t\tAfter being matched to a tutor, we will arrange a\n\t\t\t\t\t\t\tsession date and time. After this we will ask you\n\t\t\t\t\t\t\tfor payment, you can choose to pay for a single\n\t\t\t\t\t\t\tsession at a time, but it is more convenient to pay\n\t\t\t\t\t\t\tfor a few sessions ahead of time. All payments are\n\t\t\t\t\t\t\tdone through <i>Stripe</i>, which is a company that\n\t\t\t\t\t\t\thandles online transactions and are trusted by\n\t\t\t\t\t\t\tbillions.\n\t\t\t\t\t\t</Typography>\n\n\t\t\t\t\t\t<br></br>\n\n\t\t\t\t\t\t<div style={{ display: 'flex' }}>\n\t\t\t\t\t\t\t<Typography variant=\"h3\" color=\"secondary\">\n\t\t\t\t\t\t\t\t3.\n\t\t\t\t\t\t\t</Typography>\n\t\t\t\t\t\t\t<Typography variant=\"h3\">Repeat</Typography>\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t\tAfter you have finished your sessions, we will again\n\t\t\t\t\t\t\task for payment if you choose to remain with us,\n\t\t\t\t\t\t\tthis is all that needs to be done to enjoy our\n\t\t\t\t\t\t\tamazing services.\n\t\t\t\t\t\t</Typography>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t);\n};\nexport default HowItWorks;\n","/home/johnc/Code/RealTutorMERN/client/src/components/pages/Team.js",["269"],"import react from 'react';\n\nimport { makeStyles } from '@material-ui/styles';\n\nimport DefaultAppBar from '../general/DefaultAppBar';\nimport Banner from '../StyledComponents/Banner';\nimport { Typography } from '@material-ui/core';\n\nimport Section from '../StyledComponents/Section';\n\nimport John from '../../images/People/john.jpg';\nimport Zach from '../../images/People/zach.jpg';\nimport Ronnie from '../../images/People/ronnie.jpg';\nimport Alex from '../../images/People/alex.jpg';\nimport Rio from '../../images/People/rio.jpg';\n\nconst Team = () => {\n\tconst useStyles = makeStyles((theme) => ({\n\t\ttutorPicturesWrapper: {\n\t\t\twidth: '100%',\n\t\t\theight: 500,\n\t\t\tdisplay: 'flex',\n\t\t\tflexDirection: 'row',\n\t\t\tfloat: 'left',\n\t\t\t[theme.breakpoints.down('md')]: {\n\t\t\t\tmarginLeft: 0,\n\t\t\t},\n\t\t\t[theme.breakpoints.down('xs')]: {\n\t\t\t\tflexDirection: 'column',\n\t\t\t},\n\t\t},\n\t\ttutorPictures: {\n\t\t\twidth: '100%',\n\t\t\theight: '100%',\n\t\t\tbackgroundSize: 'contain',\n\t\t\tbackgroundRepeat: 'no-repeat',\n\t\t\tbackgroundPositionY: 'center',\n\t\t\tfloat: 'left',\n\t\t},\n\t}));\n\n\tconst classes = useStyles();\n\n\treturn (\n\t\t<div>\n\t\t\t<DefaultAppBar />\n\t\t\t<Banner\n\t\t\t\tsmall={true}\n\t\t\t\ttitle=\"Meet the team\"\n\t\t\t\tsubtitle=\"A small, close team, working to provide the best tutoring\"\n\t\t\t/>\n\t\t\t<Section medium={true} centered={true} background=\"offwhite\">\n\t\t\t\t<div style={{ flex: 1, width: '100%' }}>\n\t\t\t\t\t<Typography variant=\"h1\">John Costa</Typography>\n\t\t\t\t\t<div className={classes.tutorPicturesWrapper}>\n\t\t\t\t\t\t<div\n\t\t\t\t\t\t\tclassName={classes.tutorPictures}\n\t\t\t\t\t\t\tstyle={{ backgroundImage: `url(${John})` }}\n\t\t\t\t\t\t></div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div style={{ flex: 1 }}>\n\t\t\t\t\t<Typography variant=\"h3\">About me</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\tI am taking a Computer Science degree at Royal Holloway,\n\t\t\t\t\t\tI am also the CEO of Real Tutor. Maths has been my\n\t\t\t\t\t\tfavourite subject until I met computer science but it\n\t\t\t\t\t\tstill holds a crutial place in my heart. I have an A* in\n\t\t\t\t\t\tA-level Maths, an A in Further Maths and an A in\n\t\t\t\t\t\tComputer Science, as well as years of tutoring\n\t\t\t\t\t\texperience.\n\t\t\t\t\t</Typography>\n\n\t\t\t\t\t<br></br>\n\n\t\t\t\t\t<Typography variant=\"h3\">Roles</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t1. CEO of the company\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t2. Code all of the companies systems\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t3. Teach Maths, Further Maths and Computer Science\n\t\t\t\t\t</Typography>\n\t\t\t\t</div>\n\t\t\t</Section>\n\n\t\t\t<Section medium={true} centered={true} background=\"lightteal\">\n\t\t\t\t<div style={{ flex: 1, width: '100%' }}>\n\t\t\t\t\t<Typography variant=\"h1\">Zach Seth</Typography>\n\t\t\t\t\t<div className={classes.tutorPicturesWrapper}>\n\t\t\t\t\t\t<div\n\t\t\t\t\t\t\tclassName={classes.tutorPictures}\n\t\t\t\t\t\t\tstyle={{ backgroundImage: `url(${Zach})` }}\n\t\t\t\t\t\t></div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div style={{ flex: 1 }}>\n\t\t\t\t\t<Typography variant=\"h3\">About me</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\tI currently work as a Risk Analyst in the financial\n\t\t\t\t\t\tsector. I am the COO of Real tutor, a company I\n\t\t\t\t\t\tco-founded with John. We noticed a gap in the market for\n\t\t\t\t\t\thigh quality tutoring and didn't hesitate to take the\n\t\t\t\t\t\topportunity. My business mindset came from my love of\n\t\t\t\t\t\tEconomics, in which I achieved an A at Farnborough\n\t\t\t\t\t\tSixth-form college. I enjoyed reading beyond the\n\t\t\t\t\t\tcurriculum and continue to read about finance and\n\t\t\t\t\t\teconomics now. I would like to share my mindset with\n\t\t\t\t\t\tstudents by developing their understanding of the world\n\t\t\t\t\t\tof finance.\n\t\t\t\t\t</Typography>\n\n\t\t\t\t\t<br></br>\n\n\t\t\t\t\t<Typography variant=\"h3\">Roles</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t1. COO of the company\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t2. Looks after all company finances\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t3. Teach A-level Economics\n\t\t\t\t\t</Typography>\n\t\t\t\t</div>\n\t\t\t</Section>\n\n\t\t\t<Section medium={true} centered={true} background=\"offwhite\">\n\t\t\t\t<div style={{ flex: 1, width: '100%' }}>\n\t\t\t\t\t<Typography variant=\"h1\">Ronnie Nobbs</Typography>\n\t\t\t\t\t<div className={classes.tutorPicturesWrapper}>\n\t\t\t\t\t\t<div\n\t\t\t\t\t\t\tclassName={classes.tutorPictures}\n\t\t\t\t\t\t\tstyle={{ backgroundImage: `url(${Ronnie})` }}\n\t\t\t\t\t\t></div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div style={{ flex: 1 }}>\n\t\t\t\t\t<Typography variant=\"h3\">About me</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\tI am an undergraduate student at Southampton University,\n\t\t\t\t\t\ta part of the Russell Group, and I am working towards a\n\t\t\t\t\t\tMasters Degree in Physics with Mathematics. I have an\n\t\t\t\t\t\tA* in Maths, an A in Physics and a B in Further Maths,\n\t\t\t\t\t\tcoupled with experience in tutoring Physics and Maths.\n\t\t\t\t\t\tPhysics was unlocked as a passion of mine once I started\n\t\t\t\t\t\tmy GCSE learning in Year 10. I would research topics far\n\t\t\t\t\t\tbeyond the scope of the GCSE course. It lit a fire\n\t\t\t\t\t\tunderneath me, a burning passion to grow my knowledge, a\n\t\t\t\t\t\tconquest in life that will never end. Mathematics\n\t\t\t\t\t\tnaturally followed on from my passion in physics, for it\n\t\t\t\t\t\tis the language of the universe.\n\t\t\t\t\t</Typography>\n\n\t\t\t\t\t<br></br>\n\n\t\t\t\t\t<Typography variant=\"h3\">Roles</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t1. Teach A-level and GCSE Physics\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t2. Teach A-level Maths and Further Maths\n\t\t\t\t\t</Typography>\n\t\t\t\t</div>\n\t\t\t</Section>\n\n\t\t\t<Section medium={true} centered={true} background=\"lightblue\">\n\t\t\t\t<div style={{ flex: 1, width: '100%' }}>\n\t\t\t\t\t<Typography variant=\"h1\">Alex Field</Typography>\n\t\t\t\t\t<div className={classes.tutorPicturesWrapper}>\n\t\t\t\t\t\t<div\n\t\t\t\t\t\t\tclassName={classes.tutorPictures}\n\t\t\t\t\t\t\tstyle={{ backgroundImage: `url(${Alex})` }}\n\t\t\t\t\t\t></div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div style={{ flex: 1 }}>\n\t\t\t\t\t<Typography variant=\"h3\">About me</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\tI am an undergraduate studying medical biosciences at\n\t\t\t\t\t\tImperial College London. I studied Biology, Chemistry\n\t\t\t\t\t\tand maths at A level achieving an A* in Maths and an A\n\t\t\t\t\t\tgrade in biology and chemistry. I have always had a\n\t\t\t\t\t\tstrong passion for the biological sciences and often\n\t\t\t\t\t\tresearched topics beyond the spectrum of what was\n\t\t\t\t\t\tnecessary for the course. I have always had a strong\n\t\t\t\t\t\tability in the sciences and achieved the top grades at\n\t\t\t\t\t\tGCSE level and am continuing to show great ability after\n\t\t\t\t\t\tbeing accepted into Imperial College.\n\t\t\t\t\t</Typography>\n\n\t\t\t\t\t<br></br>\n\n\t\t\t\t\t<Typography variant=\"h3\">Roles</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t1. Teaching A level and GCSE Biology\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t2. Teaching A level and GCSE Chemistry\n\t\t\t\t\t</Typography>\n\t\t\t\t</div>\n\t\t\t</Section>\n\n\t\t\t<Section medium={true} centered={true} background=\"offwhite\">\n\t\t\t\t<div style={{ flex: 1, width: '100%' }}>\n\t\t\t\t\t<Typography variant=\"h1\">Rio Keefe</Typography>\n\t\t\t\t\t<div className={classes.tutorPicturesWrapper}>\n\t\t\t\t\t\t<div\n\t\t\t\t\t\t\tclassName={classes.tutorPictures}\n\t\t\t\t\t\t\tstyle={{ backgroundImage: `url(${Rio})` }}\n\t\t\t\t\t\t></div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div style={{ flex: 1 }}>\n\t\t\t\t\t<Typography variant=\"h3\">About Me</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\tI am a student of Illustration at UCA Farnham. I also\n\t\t\t\t\t\tstudied Art and Graphics in school and Sixth Form,\n\t\t\t\t\t\tachieving an A in Art and Design at A-Level and an A* in\n\t\t\t\t\t\tGraphic Products at GCSE. I used my gap year to develop\n\t\t\t\t\t\tmy portfolio and grow my online presence; I started my\n\t\t\t\t\t\tsmall business on Etsy where I sell illustration prints\n\t\t\t\t\t\tand hand-made stickers. I have always taken art\n\t\t\t\t\t\tseriously, despite its reputation as an easy subject,\n\t\t\t\t\t\tand use it to express myself. I strongly believe art\n\t\t\t\t\t\tcannot be filtered and should fully represent you and\n\t\t\t\t\t\tyour vision for it, embrace your individuality in every\n\t\t\t\t\t\taspect of your life - especially in your artwork.\n\t\t\t\t\t</Typography>\n\n\t\t\t\t\t<br></br>\n\n\t\t\t\t\t<Typography variant=\"h3\">Roles</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t1. Head of Graphic design\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t2. Teaching A-Level Art and Design\n\t\t\t\t\t</Typography>\n\t\t\t\t\t<Typography variant=\"body2\">\n\t\t\t\t\t\t3. Teaching GCSE Graphics\n\t\t\t\t\t</Typography>\n\t\t\t\t</div>\n\t\t\t</Section>\n\t\t</div>\n\t);\n};\n\nexport default Team;\n","/home/johnc/Code/RealTutorMERN/client/src/components/StyledComponents/Section.js",["270","271","272","273","274","275"],"/home/johnc/Code/RealTutorMERN/client/src/components/subjects/Alevelphysics.js",["276","277"],"/home/johnc/Code/RealTutorMERN/client/src/components/subjects/Alevelcomputerscience.js",["278","279"],"/home/johnc/Code/RealTutorMERN/client/src/components/subjects/Alevelfurthermaths.js",["280","281"],"/home/johnc/Code/RealTutorMERN/client/src/components/subjects/AlevelEconomics.js",["282","283"],"/home/johnc/Code/RealTutorMERN/client/src/components/subjects/Alevelart.js",["284","285"],"/home/johnc/Code/RealTutorMERN/client/src/components/subjects/GCSEcomputerscience.js",["286","287"],"/home/johnc/Code/RealTutorMERN/client/src/components/subjects/GCSEphysics.js",["288","289"],"/home/johnc/Code/RealTutorMERN/client/src/components/pages/Payment.js",["290","291","292"],"import react, { useState } from 'react';\n\nimport DefaultAppBar from '../general/DefaultAppBar';\nimport Banner from '../StyledComponents/Banner';\nimport { Typography } from '@material-ui/core';\nimport TextField from '@material-ui/core/TextField';\nimport Button from '@material-ui/core/Button';\nimport { loadStripe } from '@stripe/stripe-js';\n\nimport Section from '../StyledComponents/Section';\n\nconst stripePromise = loadStripe(\n\t'pk_live_51IgsTHJwHQjcU66CgZtNbj7hpXzxLlnlk9V2rqRp1M110TAUe4V4XCmPnpXrhFRCYuySROcq4UU1UXMTSAaeISnq002CgxVgr8'\n);\n\nconst Payment = () => {\n\tconst [stripeError, setStripeError] = useState();\n\tconst [loading, setLoading] = useState(false);\n\n\tcons